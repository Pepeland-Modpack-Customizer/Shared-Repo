generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id          Int       @id @unique @default(autoincrement())
  displayName String
  sessions    Session[]
}

model Session {
  sessionId String   @id @unique
  userId    Int
  user      User     @relation(fields: [userId], references: [id])
  userAgent String
  expiresIn DateTime
}
